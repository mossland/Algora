{
  "name": "@algora/model-router",
  "version": "0.1.0",
  "description": "LLM Model Router with difficulty-based routing for Algora v2.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@algora/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "algora",
    "llm",
    "model-router",
    "difficulty-classification",
    "rag",
    "embeddings"
  ],
  "license": "MIT"
}
